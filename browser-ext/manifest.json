{
    "name": "Extension SQL Injection Detection",
    "description": "Building Chrome Extension to detect SQL Injection",
    "manifest_version": 3,
    "version": "0.1",
    "icons": {
        "16": "./images/icon_16.png",
        "32": "./images/icon_32.png",
        "48": "./images/icon_48.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["./bundle.js"]
        }
    ]
}